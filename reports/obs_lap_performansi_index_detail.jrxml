	<parameter name="DYNAMIC_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_SAFE\") AS SUM_SAFE, \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_UNSAFE\") AS SUM_UNSAFE \n" +
"FROM \n" +
"     \"dbo\".\"T_STOP\" T_STOP INNER JOIN \"dbo\".\"T_STOP_DETAIL\" T_STOP_DETAIL ON T_STOP.\"ID_STOP\" = T_STOP_DETAIL.\"ID_STOP\" \n" +
"     INNER JOIN \"dbo\".\"R_SEKSI\" R_SEKSI ON T_STOP.\"ID_AREA_SEKSI\" = R_SEKSI.\"ID_SEKSI\" \n" +
"     AND R_SEKSI.\"ID_DIREKTORAT\" = T_STOP.\"ID_AREA_DIREKTORAT\" \n" +
"     AND R_SEKSI.\"ID_DINAS\" = T_STOP.\"ID_AREA_DINAS\" \n" +
"     AND R_SEKSI.\"ID_DIVPROSBU\" = T_STOP.\"ID_AREA_DIVPROSBU\" \n" +
"     AND R_SEKSI.\"ID_DEPARTEMEN\" = T_STOP.\"ID_AREA_DEPARTEMEN\" \n" +
"WHERE \n" +
"     T_STOP.\"STATUS_AKTIF\" = 1 \n" +
" AND T_STOP.\"MONTH\" = " + $P{MONTH} + "\n" +
" AND T_STOP.\"YEAR\" = " + $P{YEAR} + "\n" + $P{ORG_WIL_CLAUSE}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     sum(T_STOP_DETAIL."JUMLAH_SAFE") AS SUM_SAFE,
     sum(T_STOP_DETAIL."JUMLAH_UNSAFE") AS SUM_UNSAFE
FROM
     "dbo"."T_STOP" T_STOP INNER JOIN "dbo"."T_STOP_DETAIL" T_STOP_DETAIL ON T_STOP."ID_STOP" = T_STOP_DETAIL."ID_STOP"
WHERE
     T_STOP."YEAR" = $P{YEAR}
 AND T_STOP."STATUS_AKTIF" = 1
 AND T_STOP."MONTH" = $P{MONTH}]]>
	</queryString>
