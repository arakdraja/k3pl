"                   ,	R_PEGAWAI		as y " +
"               where	x.NIPG              = y.NIPG " +
"               and		y.STATUS_PEGAWAI    = '1' " +
"               and		x.STATUS            in (1,2) " +
"               and		x.STATUS_AKTIF      = '1' " +
"               and		YEAR(x.TANGGAL)		= " + $P{YEAR} +
"               and		MONTH(x.TANGGAL)    = " + $P{MONTH} +
"               and     x.id_direktorat     = a.id_direktorat " +
