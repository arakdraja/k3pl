"                           when	5	then isnull(SUM(x.MAY),1) " +
"                           when	6	then isnull(SUM(x.JUN),1) " +
"                           when	7	then isnull(SUM(x.JUL),1) " +
"                           when	8	then isnull(SUM(x.AUG),1) " +
"                           when	9	then isnull(SUM(x.SEP),1) " +
"                           when	10	then isnull(SUM(x.OCT),1) " +
