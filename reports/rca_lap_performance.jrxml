	<parameter name="DYNAMIC_ORG_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT "+ $P{ORG_SELECT_CLAUSE} +
"     ( SELECT  "+ $P{MONTH_CLAUSE} +" AS TOTAL " +
"       FROM    R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       T_RCA_TARGET_PEGAWAI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   R_PEGAWAI.NIPG              = T_RCA_TARGET_PEGAWAI.NIPG " +
"       AND     T_RCA_TARGET_PEGAWAI.YEAR   = " + $P{YEAR} +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = 1"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TARGET," +
"     ( SELECT count (distinct (T_RCA_AUDITOR.NIPG)) AS TOTAL_PART " +
"       FROM    T_RCA " +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI " +
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS PARTISIPASI, " +
"     ( SELECT count (T_RCA_AUDITOR.NIPG) AS TOTAL_PART " +
"       FROM    T_RCA " +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI " +
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TOTAL_PART, " +
"     ( SELECT  isnull(sum (T_RCA_DETAIL.NUMBER_OF_VIOLATIONS), 0) AS SUM_OF_VIOLATIONS " +
"       FROM    T_RCA_DETAIL " +
"       ,       T_RCA "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA_DETAIL.ID_RCA         = T_RCA.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS VIOLATION," +
"     ( SELECT  isnull(sum (T_RCA_DETAIL.NUMBER_OF_VIOLATIONS), 0) AS COUNT_OF_TEMUAN "+
"       FROM    T_RCA_DETAIL " +
"       ,       T_RCA "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA_DETAIL.ID_RCA         = T_RCA.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (4,5)" +
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN, " +
"     ( SELECT  isnull(sum (T_RCA_DETAIL.NUMBER_OF_VIOLATIONS), 0) AS COUNT_OF_TEMUAN "+
"       FROM    T_RCA_DETAIL " +
"       ,       T_RCA "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA_DETAIL.ID_RCA         = T_RCA.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    = 3" +
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN_3, " +
"     ( SELECT  isnull(sum (T_RCA_DETAIL.NUMBER_OF_VIOLATIONS), 0) AS COUNT_OF_TEMUAN "+
"       FROM    T_RCA_DETAIL " +
"       ,       T_RCA "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA_DETAIL.ID_RCA         = T_RCA.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    NOT IN (3,4,5)" +
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN_NON, " +
"     ( SELECT  isnull(sum (T_RCA_DETAIL.NUMBER_OF_VIOLATIONS), 0) AS JUMLAH "+
"       FROM    T_RCA_DETAIL " +
"       ,       T_RCA "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       ,       __USER "+
"       ,       __USER_GRUP "+
"       ,       __GRUP_USER "+
"       WHERE   T_RCA_DETAIL.ID_RCA         = T_RCA.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.NIPG              = __USER.NIPG "+
"       AND     __USER.STATUS_USER          = 1 "+
"       AND     __USER.NIPG                 = __USER_GRUP.NIPG "+
"       AND     __USER_GRUP.ID_GRUP         = __GRUP_USER.ID_GRUP "+
"       AND     __GRUP_USER.ID_GRUP         = 5 "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (4,5)" +
"       AND     T_RCA_DETAIL.STATUS         IN ('2','3')" +
        $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
