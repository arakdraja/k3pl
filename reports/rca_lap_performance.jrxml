	<parameter name="DYNAMIC_ORG_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" + $P{ORG_SELECT_CLAUSE} +
"     ( SELECT sum( \n" + $P{MONTH_CLAUSE} +
"                    ) AS TOTAL \n" +
"       FROM \n" +
"           \"dbo\".\"R_PEGAWAI\" R_PEGAWAI INNER JOIN \n" +
"               \"dbo\".\"T_RCA_TARGET_PEGAWAI\" T_RCA_TARGET_PEGAWAI \n" +
"               ON R_PEGAWAI.\"NIPG\" = T_RCA_TARGET_PEGAWAI.\"NIPG\" \n" +
"       WHERE \n" +
"           T_RCA_TARGET_PEGAWAI.\"YEAR\" = " + $P{YEAR} + "\n" +
"       AND R_PEGAWAI.\"STATUS_PEGAWAI\" = 1 \n" + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TARGET, \n" +
"     ( SELECT count(T_RCA_AUDITOR.\"NIPG\") AS TOTAL_PART \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_AUDITOR\" T_RCA_AUDITOR \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_AUDITOR.\"ID_RCA\" INNER JOIN \n" +
"               \"dbo\".\"R_PEGAWAI\" R_PEGAWAI \n" +
"               ON T_RCA_AUDITOR.\"NIPG\" = R_PEGAWAI.\"NIPG\" \n" +
"       WHERE \n" +
"           T_RCA_AUDITOR.\"STATUS\" IN (1, 2) \n" + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} + $P{PART_MONTH_CLAUSE} + "\n" +
"     ) AS PARTISIPASI, \n" +
"     ( SELECT sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\") AS SUM_OF_VIOLATIONS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS VIOLATION, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS COUNT_OF_TEMUAN \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN, \n" +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"LI_45\") is null then 0 else (1.0 * (sum(T_RCA_DETAIL.\"LI_45\") * 100) / isnull(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"),1)) end) as varchar) AS SEVERITY_PERCENTAGE \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS SEVERITY, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS JUMLAH " +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND T_RCA_DETAIL.\"STATUS\" IN (2, 3) \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_45, \n" +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") is null then 0 else (1.0 * sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") / isnull(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"),1)) end) as varchar) AS SUM_OF_VXS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" +
"               \"dbo\".\"T_RCA_AUDITOR\" T_RCA_AUDITOR \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_AUDITOR.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS VXS \n" +
"FROM \n" + $P{ORG_FROM_CLAUSE} + " R_ \n" +
"WHERE \n" +
"     1 = 1 \n" + $P{DIR_CLAUSE} + $P{DIV_CLAUSE} + $P{DEP_CLAUSE} + $P{DINAS_CLAUSE} + $P{SEKSI_CLAUSE} +
" ORDER BY \n" + $P{ORG_ORDER_CLAUSE} + " ASC"]]></defaultValueExpression>
	</parameter>
