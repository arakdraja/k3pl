	<parameter name="WILAYAH_TARGET_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" )
?   "   AND R_SEKSI.ID_SEKSI = "+ $P{ID_AREA}
:   $P{ID_WILAYAH} != null && ! $P{ID_WILAYAH}.isEmpty() && ! $P{ID_WILAYAH}.equalsIgnoreCase( "0" )
    ?   "   AND R_SEKSI.ID_WILAYAH  = "+ $P{ID_WILAYAH}
    :   ""]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_SELECT_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" ) ?
    "     R_.ID_SEKSI AS ID, " +
    "     R_.NAMA_SEKSI AS NAMA, " :
$P{ID_WILAYAH} != null && ! $P{ID_WILAYAH}.isEmpty() && ! $P{ID_WILAYAH}.equalsIgnoreCase( "0" ) ?
    "     R_.\"ID_SEKSI\" AS ID, \n" +
    "     R_.\"NAMA_SEKSI\" AS NAMA, \n" :
    "     R_.\"ID_WILAYAH\" AS ID, \n" +
    "     R_.\"NAMA_WILAYAH\" AS NAMA, \n"]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_FROM_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" )
?    "  R_SEKSI R_"
:   $P{ID_WILAYAH} != null && ! $P{ID_WILAYAH}.isEmpty() && ! $P{ID_WILAYAH}.equalsIgnoreCase( "0" )
    ?    "  R_SEKSI R_ "
    :    "  R_WILAYAH R_ "]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_ORDER_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" ) ?
    "     R_.\"NAMA_PEGAWAI\" " :
$P{ID_WILAYAH} != null && ! $P{ID_WILAYAH}.isEmpty() && ! $P{ID_WILAYAH}.equalsIgnoreCase( "0" ) ?
    "     R_.\"ID_SEKSI\" " :
    "     R_.\"ID_WILAYAH\" "]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_NIPG3_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["               AND R_S.\"ID_DIREKTORAT\" = R_PEGAWAI.\"ID_DIREKTORAT\" \n" +
"               AND R_S.\"ID_DINAS\" = R_PEGAWAI.\"ID_DINAS\" \n" +
"               AND R_S.\"ID_DIVPROSBU\" = R_PEGAWAI.\"ID_DIVPROSBU\" \n" +
"               AND R_S.\"ID_DEPARTEMEN\" = R_PEGAWAI.\"ID_DEPARTEMEN\" \n"]]></defaultValueExpression>
	</parameter>
	<parameter name="DYNAMIC_WIL_QUERY" class="java.lang.String" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA["SELECT " + $P{WIL_SELECT_CLAUSE} +
"     ( SELECT  "+ $P{MONTH_CLAUSE} +" AS TOTAL " +
"       FROM    R_PEGAWAI "+
"       ,       T_RCA_TARGET_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   R_PEGAWAI.NIPG              = T_RCA_TARGET_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1' "+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     T_RCA_TARGET_PEGAWAI.YEAR   = "+ $P{YEAR} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TARGET, " +
"     ( SELECT  count(distinct(T_RCA_AUDITOR.NIPG)) AS TOTAL_PARTISIPAN " +
"       FROM    T_RCA" +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI " +
"       AND     year (T_RCA.TANGGAL_RCA )   = " + $P{YEAR} +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS PARTISIPASI, " +
"     ( SELECT  count(T_RCA_AUDITOR.NIPG)   AS TOTAL_PART " +
"       FROM    T_RCA" +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI " +
"       AND     year (T_RCA.TANGGAL_RCA )   = " + $P{YEAR} +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TOTAL_PART, " +
"     ( SELECT  sum(T_RCA_DETAIL.NUMBER_OF_VIOLATIONS) AS SUM_OF_VIOLATIONS " +
"       FROM    T_RCA "+
"       ,       T_RCA_DETAIL "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA "+
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS VIOLATION, " +
"     ( SELECT  count(T_RCA_DETAIL.ID_RCA) AS COUNT_OF_TEMUAN " +
"       FROM    T_RCA " +
"       ,       T_RCA_DETAIL " +
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (4, 5) " +
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TEMUAN, " +
"     ( SELECT  isnull (nullif (count(T_RCA_DETAIL.ID_RCA), 0), 1) AS COUNT_OF_TEMUAN " +
"       FROM    T_RCA " +
"       ,       T_RCA_DETAIL " +
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (1, 2, 3) " +
"       AND year(T_RCA.TANGGAL_RCA)         = " + $P{YEAR} +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TEMUAN_NON, " +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.LI_45) is null then 0 else (1.0 * (sum(T_RCA_DETAIL.LI_45) * 100) / isnull(nullif(sum(T_RCA_DETAIL.NUMBER_OF_VIOLATIONS),0),1)) end) as varchar) AS SEVERITY_PERCENTAGE " +
"       FROM    T_RCA "+
"       ,       T_RCA_DETAIL "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG " +
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS SEVERITY, " +
"     ( SELECT count(T_RCA_DETAIL.ID_RCA) AS JUMLAH " +
"       FROM    T_RCA "+
"       ,       T_RCA_DETAIL "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG " +
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     T_RCA_DETAIL.ID_SEVERITY       IN (4, 5) " +
"       AND     T_RCA_DETAIL.STATUS         IN ('2', '3') " +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_45, " +
"     ( SELECT count(T_RCA_DETAIL.ID_RCA) AS JUMLAH " +
"       FROM    T_RCA "+
"       ,       T_RCA_DETAIL "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG " +
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (1, 2, 3) " +
"       AND     T_RCA_DETAIL.STATUS         IN ('2', '3') " +
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_NON_45, " +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.VIOLATION_X_SEVERITY) is null then 0 else (1.0 * sum(T_RCA_DETAIL.VIOLATION_X_SEVERITY) / isnull(nullif(sum(T_RCA_DETAIL.NUMBER_OF_VIOLATIONS),0),1)) end) as varchar) AS SUM_OF_VXS " +
"       FROM    T_RCA "+
"       ,       T_RCA_DETAIL "+
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG " +
"       AND     year(T_RCA.TANGGAL_RCA)     = "+ $P{YEAR} +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_SEKSI.ID_SEKSI "+
        $P{PART_MONTH_CLAUSE} +
        $P{WILAYAH_TARGET_CLAUSE} +
"     ) AS VXS " +
"FROM " + $P{WIL_FROM_CLAUSE} + " " +
"WHERE " +
"     1 = 1 " + $P{WILAYAH_CLAUSE} +
" ORDER BY NAMA ASC"]]></defaultValueExpression>
	</parameter>
	<parameter name="MAIN_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{IS_IN_ORG} != null &&
! $P{IS_IN_ORG}.isEmpty() &&
$P{IS_IN_ORG}.equalsIgnoreCase( "0" ) ?
$P{DYNAMIC_WIL_QUERY} :
$P{DYNAMIC_ORG_QUERY}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[$P!{MAIN_QUERY}]]>
	</queryString>
