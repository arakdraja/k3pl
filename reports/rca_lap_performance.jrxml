    "     \"dbo\".\"R_WILAYAH\" R_ "]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_ORDER_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" ) ?
    "     R_.\"NAMA_PEGAWAI\" " :
$P{ID_WILAYAH} != null && ! $P{ID_WILAYAH}.isEmpty() && ! $P{ID_WILAYAH}.equalsIgnoreCase( "0" ) ?
    "     R_.\"ID_SEKSI\" " :
    "     R_.\"ID_WILAYAH\" "]]></defaultValueExpression>
	</parameter>
	<parameter name="WIL_NIPG3_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["               \"dbo\".\"R_PEGAWAI\" R_PEGAWAI \n" +
"               ON T_RCA.\"PENANGGUNG_JAWAB_NIPG\" = R_PEGAWAI.\"NIPG\" INNER JOIN \n" +
"               \"dbo\".\"R_SEKSI\" R_S \n" +
"               ON R_PEGAWAI.\"ID_SEKSI\" = R_S.\"ID_SEKSI\" \n" +
"               AND R_S.\"ID_DIREKTORAT\" = R_PEGAWAI.\"ID_DIREKTORAT\" \n" +
"               AND R_S.\"ID_DINAS\" = R_PEGAWAI.\"ID_DINAS\" \n" +
"               AND R_S.\"ID_DIVPROSBU\" = R_PEGAWAI.\"ID_DIVPROSBU\" \n" +
"               AND R_S.\"ID_DEPARTEMEN\" = R_PEGAWAI.\"ID_DEPARTEMEN\" \n"]]></defaultValueExpression>
	</parameter>
	<parameter name="DYNAMIC_WIL_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" + $P{WIL_SELECT_CLAUSE} +
"     ( SELECT sum( \n" + $P{MONTH_CLAUSE} +
"                    ) AS TOTAL \n" +
"       FROM \n" +
"           \"dbo\".\"R_PEGAWAI\" R_PEGAWAI INNER JOIN \n" +
"               \"dbo\".\"T_RCA_TARGET_PEGAWAI\" T_RCA_TARGET_PEGAWAI \n" +
"               ON R_PEGAWAI.\"NIPG\" = T_RCA_TARGET_PEGAWAI.\"NIPG\" \n" +
"               INNER JOIN \"dbo\".\"R_SEKSI\" R_S \n" +
"               ON R_PEGAWAI.\"ID_SEKSI\" = R_S.\"ID_SEKSI\" \n" +
"       WHERE \n" +
"           T_RCA_TARGET_PEGAWAI.\"YEAR\" = " + $P{YEAR} + "\n" +
"       AND R_PEGAWAI.\"STATUS_PEGAWAI\" = 1 \n" + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS TARGET, \n" +
"     ( SELECT count(T_RCA_AUDITOR.\"NIPG\") AS TOTAL_PART \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_AUDITOR\" T_RCA_AUDITOR \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_AUDITOR.\"ID_RCA\" INNER JOIN \n" +
"               \"dbo\".\"R_PEGAWAI\" R_PEGAWAI \n" +
"               ON T_RCA_AUDITOR.\"NIPG\" = R_PEGAWAI.\"NIPG\" \n" +
"               INNER JOIN \"dbo\".\"R_SEKSI\" R_S \n" +
"               ON R_PEGAWAI.\"ID_SEKSI\" = R_S.\"ID_SEKSI\" \n" +
"       WHERE \n" +
"           T_RCA_AUDITOR.\"STATUS\" IN (1, 2) \n" + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} + "\n" +
"     ) AS PARTISIPASI, \n" +
"     ( SELECT sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\") AS SUM_OF_VIOLATIONS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{WIL_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS VIOLATION, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS COUNT_OF_TEMUAN \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{WIL_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS TEMUAN, \n" +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"LI_45\") is null then 0 else (1.0 * (sum(T_RCA_DETAIL.\"LI_45\") * 100) / isnull(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"),1)) end) as varchar) AS SEVERITY_PERCENTAGE \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{WIL_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS SEVERITY, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS JUMLAH " +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{WIL_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND T_RCA_DETAIL.\"STATUS\" IN (2, 3) \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_45, \n" +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") is null then 0 else (1.0 * sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") / isnull(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"),1)) end) as varchar) AS SUM_OF_VXS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" +
"               \"dbo\".\"T_RCA_AUDITOR\" T_RCA_AUDITOR \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_AUDITOR.\"ID_RCA\" INNER JOIN \n" + $P{WIL_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{WILAYAH_TARGET_CLAUSE} + $P{AREA_TARGET_CLAUSE} +
"     ) AS VXS \n" +
"FROM \n" + $P{WIL_FROM_CLAUSE} + " \n" +
"WHERE \n" +
"     1 = 1 \n" + $P{WILAYAH_CLAUSE} + $P{AREA_CLAUSE} +
" ORDER BY \n" + $P{WIL_ORDER_CLAUSE} + " ASC"]]></defaultValueExpression>
	</parameter>
	<parameter name="MAIN_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{IS_IN_ORG} != null &&
! $P{IS_IN_ORG}.isEmpty() &&
$P{IS_IN_ORG}.equalsIgnoreCase( "0" ) ?
$P{DYNAMIC_WIL_QUERY} :
$P{DYNAMIC_ORG_QUERY}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[$P!{MAIN_QUERY}]]>
	</queryString>
