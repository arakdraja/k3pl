	<parameter name="DYNAMIC_ORG_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT "+ $P{ORG_SELECT_CLAUSE} +
"     ( SELECT      "+ $P{MONTH_CLAUSE} +" AS TOTAL " +
"           FROM    R_PEGAWAI "+
"           ,       T_RCA_TARGET_PEGAWAI "+
"           WHERE   R_PEGAWAI.NIPG                  = T_RCA_TARGET_PEGAWAI.NIPG " +
"           AND     T_RCA_TARGET_PEGAWAI.YEAR       = " + $P{YEAR} +
"           AND     R_PEGAWAI.STATUS_PEGAWAI        = 1"+
            $P{DIR_TARGET_CLAUSE} +
            $P{DIV_TARGET_CLAUSE} +
            $P{DEP_TARGET_CLAUSE} +
            $P{DINAS_TARGET_CLAUSE} +
            $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TARGET," +
"     ( SELECT count (distinct (T_RCA_AUDITOR.NIPG)) AS TOTAL_PART " +
"       FROM    T_RCA " +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI " +
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     year (T_RCA.TANGGAL_RCA)    = "+ $P{YEAR} + $P{PART_MONTH_CLAUSE} +
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS PARTISIPASI, " +
"     ( SELECT count (T_RCA_AUDITOR.NIPG) AS TOTAL_PART " +
"       FROM    T_RCA " +
"       ,       T_RCA_AUDITOR " +
"       ,       R_PEGAWAI " +
"       WHERE   T_RCA.ID_RCA                = T_RCA_AUDITOR.ID_RCA "+
"       AND     year (T_RCA.TANGGAL_RCA)    = "+ $P{YEAR} + $P{PART_MONTH_CLAUSE} +
"       AND     T_RCA_AUDITOR.NIPG          = R_PEGAWAI.NIPG " +
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TOTAL_PART, " +
"     ( SELECT  sum (T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\") AS SUM_OF_VIOLATIONS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS VIOLATION, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS COUNT_OF_TEMUAN \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN, \n" +
"     ( SELECT  isnull (nullif (count(T_RCA_DETAIL.ID_RCA), 0), 1) AS COUNT_OF_TEMUAN " +
"       FROM    T_RCA " +
"       ,       T_RCA_DETAIL " +
"       ,       R_PEGAWAI "+
"       ,       R_SEKSI R_S "+
"       WHERE   T_RCA.ID_RCA                = T_RCA_DETAIL.ID_RCA " +
"       AND     T_RCA.PENANGGUNG_JAWAB_NIPG = R_PEGAWAI.NIPG "+
"       AND     R_PEGAWAI.STATUS_PEGAWAI    = '1'"+
"       AND     R_PEGAWAI.ID_SEKSI          = R_S.ID_SEKSI "+
"       AND     T_RCA_DETAIL.ID_SEVERITY    IN (1, 2, 3) " +
"       AND     year(T_RCA.TANGGAL_RCA)     = " + $P{YEAR} + $P{PART_MONTH_CLAUSE} +
        $P{DIR_TARGET_CLAUSE} +
        $P{DIV_TARGET_CLAUSE} +
        $P{DEP_TARGET_CLAUSE} +
        $P{DINAS_TARGET_CLAUSE} +
        $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TEMUAN_NON, " +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"LI_45\") is null then 0 else (1.0 * (sum(T_RCA_DETAIL.\"LI_45\") * 100) / isnull(nullif(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"), 0),1)) end) as varchar) AS SEVERITY_PERCENTAGE \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS SEVERITY, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS JUMLAH " +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (4, 5) \n" +
"       AND T_RCA_DETAIL.\"STATUS\" IN ('2', '3') \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_45, \n" +
"     ( SELECT count(T_RCA_DETAIL.\"ID_RCA\") AS JUMLAH " +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           T_RCA_DETAIL.\"ID_SEVERITY\" IN (1, 2, 3) \n" +
"       AND T_RCA_DETAIL.\"STATUS\" IN ('2', '3') \n" +
"       AND year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS TL_TEMUAN_NON_45, \n" +
"     ( SELECT cast(convert(decimal(18,2), case when sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") is null then 0 else (1.0 * sum(T_RCA_DETAIL.\"VIOLATION_X_SEVERITY\") / isnull(nullif(sum(T_RCA_DETAIL.\"NUMBER_OF_VIOLATIONS\"),0),1)) end) as varchar) AS SUM_OF_VXS \n" +
"       FROM \n" +
"           \"dbo\".\"T_RCA\" T_RCA INNER JOIN \n" +
"               \"dbo\".\"T_RCA_DETAIL\" T_RCA_DETAIL \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_DETAIL.\"ID_RCA\" INNER JOIN \n" +
"               \"dbo\".\"T_RCA_AUDITOR\" T_RCA_AUDITOR \n" +
"               ON T_RCA.\"ID_RCA\" = T_RCA_AUDITOR.\"ID_RCA\" INNER JOIN \n" + $P{ORG_NIPG3_CLAUSE} +
"       WHERE \n" +
"           year(T_RCA.\"TANGGAL_RCA\") = " + $P{YEAR} +
$P{PART_MONTH_CLAUSE} + $P{DIR_TARGET_CLAUSE} + $P{DIV_TARGET_CLAUSE} + $P{DEP_TARGET_CLAUSE} + $P{DINAS_TARGET_CLAUSE} + $P{SEKSI_TARGET_CLAUSE} +
"     ) AS VXS \n" +
"FROM \n" + $P{ORG_FROM_CLAUSE} + " R_ \n" +
"WHERE \n" +
"     1 = 1 \n" + $P{DIR_CLAUSE} + $P{DIV_CLAUSE} + $P{DEP_CLAUSE} + $P{DINAS_CLAUSE} + $P{SEKSI_CLAUSE} +
" ORDER BY NAMA ASC"]]></defaultValueExpression>
	</parameter>
	<parameter name="WILAYAH_CLAUSE" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{ID_AREA} != null && ! $P{ID_AREA}.isEmpty() && ! $P{ID_AREA}.equalsIgnoreCase( "0" )
?   "   AND R_.ID_SEKSI = " + $P{ID_AREA}
