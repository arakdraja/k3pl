	<parameter name="DYNAMIC_ORG_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_SAFE\") AS TOTAL_SAFE, \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_UNSAFE\") AS TOTAL_UNSAFE \n" +
"FROM \n" +
"     \"dbo\".\"T_STOP\" T_STOP INNER JOIN \"dbo\".\"T_STOP_DETAIL\" T_STOP_DETAIL ON T_STOP.\"ID_STOP\" = T_STOP_DETAIL.\"ID_STOP\" \n" +
"     INNER JOIN \"dbo\".\"R_SEKSI\" R_SEKSI ON T_STOP.\"ID_AREA_SEKSI\" = R_SEKSI.\"ID_SEKSI\" \n" +
"     AND R_SEKSI.\"ID_DIREKTORAT\" = T_STOP.\"ID_AREA_DIREKTORAT\" \n" +
"     AND R_SEKSI.\"ID_DINAS\" = T_STOP.\"ID_AREA_DINAS\" \n" +
"     AND R_SEKSI.\"ID_DIVPROSBU\" = T_STOP.\"ID_AREA_DIVPROSBU\" \n" +
"     AND R_SEKSI.\"ID_DEPARTEMEN\" = T_STOP.\"ID_AREA_DEPARTEMEN\" \n" +
"WHERE \n" +
"     T_STOP.\"YEAR\" = " + $P{YEAR} + "\n" +
" AND T_STOP.\"STATUS_AKTIF\" = 1 \n" + $P{MONTH_CLAUSE} + $P{DIR_CLAUSE} + $P{DIV_CLAUSE} + $P{DEP_CLAUSE} + $P{DINAS_CLAUSE} + $P{SEKSI_CLAUSE}]]></defaultValueExpression>
	</parameter>
	<parameter name="DYNAMIC_WIL_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_SAFE\") AS TOTAL_SAFE, \n" +
"     sum(T_STOP_DETAIL.\"JUMLAH_UNSAFE\") AS TOTAL_UNSAFE \n" +
"FROM \n" +
"     \"dbo\".\"T_STOP\" T_STOP INNER JOIN \"dbo\".\"T_STOP_DETAIL\" T_STOP_DETAIL ON T_STOP.\"ID_STOP\" = T_STOP_DETAIL.\"ID_STOP\" \n" +
"     INNER JOIN \"dbo\".\"R_SEKSI\" R_SEKSI ON T_STOP.\"ID_AREA_SEKSI\" = R_SEKSI.\"ID_SEKSI\" \n" +
"     AND R_SEKSI.\"ID_DIREKTORAT\" = T_STOP.\"ID_AREA_DIREKTORAT\" \n" +
"     AND R_SEKSI.\"ID_DINAS\" = T_STOP.\"ID_AREA_DINAS\" \n" +
"     AND R_SEKSI.\"ID_DIVPROSBU\" = T_STOP.\"ID_AREA_DIVPROSBU\" \n" +
"     AND R_SEKSI.\"ID_DEPARTEMEN\" = T_STOP.\"ID_AREA_DEPARTEMEN\" \n" +
"WHERE \n" +
"     T_STOP.\"YEAR\" = " + $P{YEAR} + "\n" +
" AND T_STOP.\"STATUS_AKTIF\" = 1 \n" + $P{MONTH_CLAUSE} + $P{WILAYAH_CLAUSE} + $P{AREA_CLAUSE}]]></defaultValueExpression>
	</parameter>
	<parameter name="MAIN_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{IS_IN_ORG} != null &&
! $P{IS_IN_ORG}.isEmpty() &&
$P{IS_IN_ORG}.equalsIgnoreCase( "0" ) ?
$P{DYNAMIC_WIL_QUERY} :
$P{DYNAMIC_ORG_QUERY}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[$P!{MAIN_QUERY}]]>
	</queryString>
