	<parameter name="DYNAMIC_QUERY" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["SELECT \n" +
"     V_SEVERITY.\"description\" AS V_SEVERITY_description, \n" +
"     V_SEVERITY.\"id_severity\" AS V_SEVERITY_id_severity, \n" +
"     V_SEVERITY.\"nama_tempat_rca\" AS V_SEVERITY_nama_tempat_rca, \n" +
"     V_SEVERITY.\"penanggung_jawab_seksi\" AS V_SEVERITY_penanggung_jawab_seksi, \n" +
"     R_SEKSI.\"NAMA_SEKSI\" AS R_SEKSI_NAMA_SEKSI, \n" +
"     V_SEVERITY.\"penanggung_jawab_dinas\" AS V_SEVERITY_penanggung_jawab_dinas, \n" +
"     R_DINAS.\"NAMA_DINAS\" AS R_DINAS_NAMA_DINAS, \n" +
"     V_SEVERITY.\"penanggung_jawab_departemen\" AS V_SEVERITY_penanggung_jawab_departemen, \n" +
"     R_DEPARTEMEN.\"NAMA_DEPARTEMEN\" AS R_DEPARTEMEN_NAMA_DEPARTEMEN, \n" +
"     V_SEVERITY.\"penanggung_jawab_divprosbu\" AS V_SEVERITY_penanggung_jawab_divprosbu, \n" +
"     R_DIVPROSBU.\"NAMA_DIVPROSBU\" AS R_DIVPROSBU_NAMA_DIVPROSBU, \n" +
"     V_SEVERITY.\"penanggung_jawab_direktorat\" AS V_SEVERITY_penanggung_jawab_direktorat, \n" +
"     R_DIREKTORAT.\"NAMA_DIREKTORAT\" AS R_DIREKTORAT_NAMA_DIREKTORAT, \n" +
"     V_SEVERITY.\"completion_date_target\" AS V_SEVERITY_completion_date_target, \n" +
"     V_SEVERITY.\"status\" AS V_SEVERITY_status, \n" +
"     V_SEVERITY.\"note\" AS V_SEVERITY_note, \n" +
"     V_SEVERITY.\"tanggal_rca\" AS V_SEVERITY_tanggal_rca \n" +
"FROM \n" +
"     \"dbo\".\"R_DINAS\" R_DINAS INNER JOIN \"dbo\".\"R_SEKSI\" R_SEKSI ON R_DINAS.\"ID_DINAS\" = R_SEKSI.\"ID_DINAS\" \n" +
"     AND R_DINAS.\"ID_DIREKTORAT\" = R_SEKSI.\"ID_DIREKTORAT\" \n" +
"     AND R_DINAS.\"ID_DEPARTEMEN\" = R_SEKSI.\"ID_DEPARTEMEN\" \n" +
"     AND R_DINAS.\"ID_DIVPROSBU\" = R_SEKSI.\"ID_DIVPROSBU\" \n" +
"     INNER JOIN \"dbo\".\"R_DEPARTEMEN\" R_DEPARTEMEN ON R_DINAS.\"ID_DEPARTEMEN\" = R_DEPARTEMEN.\"ID_DEPARTEMEN\" \n" +
"     AND R_DEPARTEMEN.\"ID_DIVPROSBU\" = R_DINAS.\"ID_DIVPROSBU\" \n" +
"     AND R_DEPARTEMEN.\"ID_DIREKTORAT\" = R_DINAS.\"ID_DIREKTORAT\" \n" +
"     INNER JOIN \"dbo\".\"R_DIVPROSBU\" R_DIVPROSBU ON R_DEPARTEMEN.\"ID_DIVPROSBU\" = R_DIVPROSBU.\"ID_DIVPROSBU\" \n" +
"     AND R_DIVPROSBU.\"ID_DIREKTORAT\" = R_DEPARTEMEN.\"ID_DIREKTORAT\" \n" +
"     INNER JOIN \"dbo\".\"R_DIREKTORAT\" R_DIREKTORAT ON R_DIVPROSBU.\"ID_DIREKTORAT\" = R_DIREKTORAT.\"ID_DIREKTORAT\" \n" +
"     INNER JOIN \"dbo\".\"V_SEVERITY\" V_SEVERITY ON R_SEKSI.\"ID_SEKSI\" = V_SEVERITY.\"penanggung_jawab_seksi\" \n" +
"     AND V_SEVERITY.\"penanggung_jawab_departemen\" = R_SEKSI.\"ID_DEPARTEMEN\" \n" +
"     AND V_SEVERITY.\"penanggung_jawab_direktorat\" = R_SEKSI.\"ID_DIREKTORAT\" \n" +
"     AND V_SEVERITY.\"penanggung_jawab_divprosbu\" = R_SEKSI.\"ID_DIVPROSBU\" \n" +
"     AND V_SEVERITY.\"penanggung_jawab_dinas\" = R_SEKSI.\"ID_DINAS\" \n" +
"WHERE \n" +
"     year(V_SEVERITY.\"tanggal_rca\") = " + $P{YEAR} + " \n" +
$P{MONTH_CLAUSE} + $P{ORG_WIL_CLAUSE} +
"ORDER BY \n" +
"     V_SEVERITY.\"tanggal_rca\" ASC \n"]]></defaultValueExpression>
	</parameter>
